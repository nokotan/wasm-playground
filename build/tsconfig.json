{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"lib": [
			"ES2020"
		],
		"outDir": "dist",
		"strict": true,
		"noImplicitAny": false,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
	},
	"include": [
		"**/*.ts"
	],
	"exclude": [
		"node_modules",
		"*.d.ts"
	]
}